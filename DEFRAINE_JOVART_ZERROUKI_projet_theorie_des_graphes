def build_graph_val(M_couts, graph=None):
    return nx.from_numpy_matrix(M_couts, create_using=graph)


def show(G):
    pos = nx.kamada_kawai_layout(G)  # positions for all nodes

    # nodes
    nx.draw_networkx_nodes(G, pos, node_size=300)

    # edges
    nx.draw_networkx_edges(G, pos, edgelist=G.edges(),
                           width=1)

    # labels
    # nx.draw(G, pos)
    # labels = nx.get_edge_attributes(G, 'weight')
    # nx.draw_networkx_edge_labels(G, pos, edge_labels=labels)

    nx.draw_networkx_labels(G, pos, font_size=20, font_family='sans-serif')
    nx.write_graphml(G, 'tmp.graphml')

    plt.axis('off')
    # plt.savefig("weighted_graph.png")  # save as png
    plt.show()  # display


def taches(G: nx.DiGraph) -> List[List[int]]:
    G = G.copy(1)
    taches: List[List[int]] = []
    while G.number_of_nodes() > 0:
        taches.append([])
        for node, degree in G.in_degree:
            # print(node, degree)
            if degree == 0:
                taches[-1].append(node)
        if D:
            show(G)
        G.remove_nodes_from(taches[-1])

    return taches


def main():
    G = build_graph_val(np.matrix(
        M
    ), graph=nx.DiGraph())

    print(nx.info(G))
    if not D:
        show(G)

    print("---- Début algo ----")

    taches_list = taches(G)

    for i, sublist in enumerate(taches_list):
        print("Jour", i, ":", sublist)

    print("Soit, dans l'ordre:")

    flat_list = list(itertools.chain(*taches_list))

    print(flat_list)


if __name__ == '__main__':
    main()
